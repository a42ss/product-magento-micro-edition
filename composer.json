{
  "name": "a42/product-magento-micro-edition",
  "version": "0.24.1",
  "description": "This is a fork of the magento/product-magento-micro-edition metapackage orientated to microservice architecture.",
  "type": "project",
  "license": [
    "OSL-3.0",
    "GPL-3.0-only"
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "magento/magento2-base": "2.4.3-p1",
    "magento/framework": "103.0.*",
    "colinmollenhour/cache-backend-file": "~1.4.5",
    "colinmollenhour/cache-backend-redis": "~1.14.2",
    "colinmollenhour/credis": "~1.12.1",
    "colinmollenhour/php-redis-session-abstract": "~1.4.4",
    "composer/composer": "^1.9 || ^2.0",
    "monolog/monolog": "^1.17",

    "php": "~7.3.0||~7.4.0||~8.0.0",
    "ext-bcmath": "*",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-hash": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pdo_mysql": "*",
    "ext-simplexml": "*",
    "ext-soap": "*",
    "ext-sodium": "*",
    "ext-xsl": "*",
    "ext-zip": "*",
    "lib-libxml": "*"
  },
  "require-dev": {
    "allure-framework/allure-phpunit": "~1.4",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
    "friendsofphp/php-cs-fixer": "~2.18.1",
    "lusitanian/oauth": "~0.8.10",
    "magento/magento-coding-standard": "*",
    "magento/magento2-functional-testing-framework": "^3.0",
    "pdepend/pdepend": "~2.7.1",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpmd/phpmd": "^2.8.0",
    "phpstan/phpstan": "^0.12.77",
    "phpunit/phpunit": "^9",
    "sebastian/phpcpd": "^6.0.3",
    "squizlabs/php_codesniffer": "~3.5.4",
    "symfony/finder": "^5.2"
  },
  "replace": {
    "magento/product-enterprise-edition": "*",
    "magento/product-community-edition": "*"
  },
  "conflict": {
    "gene/bluefoot": "*"
  },
  "autoload": {
  },
  "autoload-dev": {
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "extra": {
    "magento-force": "override"
  },
  "scripts": {
    "post-install-cmd": [
    ],
    "post-update-cmd": [
    ],
    "post-autoload-dump": [
    ]
  }
}
